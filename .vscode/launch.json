    "version": "0.2.0",
    "configurations": [
        
        {
            "name": "Python Debugger: Main",
            "type": "debugpy",
            "request": "launch",
            "module": "app",
            "justMyCode": false,
        },
        {
            "name": "Python Debugger: Streamlit",
            "type": "debugpy",
            "request": "launch",
            "module": "streamlit",
            "args": ["run", "app.py"],
            "justMyCode": false,
        },
        {
            "name": "Python Debugger: Generate Simulation Data",
            "type": "debugpy",
            "request": "launch",
            "module": "evaluation.generate_simulation_data",
            "subProcess": true,
            "justMyCode": false,
        }
    ]
}